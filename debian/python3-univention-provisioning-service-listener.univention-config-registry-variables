[nats/user]
Description[en]=NATS user name specific to UdmProducerSettings
Description[de]=NATS-Benutzername, der für UdmProducerSettings spezifisch ist
Categories=provisioning
Type=str

[nats/passwordfile]
Description[en]=Path to a file containing the NATS password
Description[de]=Pfad zu einer Datei, die das NATS-Passwort enthält
Categories=provisioning
Type=str

[nats/host]
Description[en]=NATS host name or IP address
Description[de]=NATS-Host-Name oder IP-Adresse
Categories=provisioning
Type=str

[nats/port]
Description[en]=NATS port number
Description[de]=NATS-Portnummer
Categories=provisioning
Type=portnumber
Default=4222

[nats/max_reconnect_attempts]
Description[en]=Maximum number of reconnect attempts to the NATS server
Description[de]=Maximale Anzahl von Wiederverbindungsversuchen zum NATS-Server
Categories=provisioning
Type=str

[nats/terminate_listener_on_exception]
Description[en]=Terminate listener on exception
Description[de]=Listener bei Ausnahme beenden
Categories=provisioning
Type=bool
Default=false

[provisioning-service/log/level]
Description[en]=Log level for the Provisioning Service
Description[de]=Log level für den Provisioning Service
Categories=provisioning
Type=str
Regex=^(ERROR|WARNING|PROCESS|INFO|DEBUG|TRACE)$
Default=WARNING
